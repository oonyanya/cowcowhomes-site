extend /layout

block custom_head
	title TOP

block custom_header
	include _header
	#main_titile
		picture
			source(media="(max-width: 768px)" srcset="/en/image/accelerate_mobile.png")
			img(src='/en/image/accelerate.png')

block content
	#infomation
		#topics
			#summary
				img(src='/img/building_house2.png')
				p #[a(href='/posts/en/rentals') RENTAL APARTMENT] Listing broking rental apartment or my company's own.
			#summary
				img(src='/img/business_icon_big_company.png')
				p #[a(href='/posts/en/pages/about.html') ABOUT] Describing location of company or telephone number and mail etc.
		#prerelease
			h2 Prerelease
			table
				tr
					td 2019/03/01
					td We make apartment list in blog space(update in the shortest one day)
				tr
					td 2016/12/12
					td We made home page
	#sidebar
		+facebook-plugin()

block custom_footer
	include _footer
