extend _page_base

block custom_header
	include _header
	#main_titile
		- if(page != undefined)
			h1 #{page.title}
	nav#bread

block content
	#infomation
		each item in page.posts.toArray()
			#summary
				a(href=`${url_for(item.path)}`) #{item.title}
		ul
			- if(page.prev)
				li a(href=`/${page.prev_link}`) Prev
			- if(page.prev)
				li a(href=`/${page.nex_link}`) Next

block custom_footer
	include _footer
